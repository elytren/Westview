<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
   <title>Westview</title>
   <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
</head>
<body>
    <header class="text-center">Westview<br> Home: Its where you make it
    </header>
   <%- include('./partials/nav.ejs') %>
    <div class="container">
        <div class="row">
            <% decs.forEach((dec)=>{ %>
                    
                        <div class="col-lg-4 col-sm-6 py-5">
                            <a href="/wv/<%= dec._id %>">
                                <div class="card bg-dark h-100" id="hexagon">
                                    <img class="img-fluid h-100 w-100"  src="<%= dec.img %>">
                                    <div class="card-img-overlay text-center text-white">
                                        <h3>
                                        <%= dec.decade %>
                                        </h3>
                                    </div>
                                </div>  
                            </a>
                        </div>
                  
            <% }) %> 
        </div>
    </div>

<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.5/dist/umd/popper.min.js" integrity="sha384-Xe+8cL9oJa6tN/veChSP7q+mnSPaj5Bcu9mPX5F5xIGE0DVittaqT5lorf0EI7Vk" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.min.js" integrity="sha384-kjU+l4N0Yf4ZOJErLsIcvOU2qSb74wXpOhqTvwVx3OElZRweTnQ6d31fXEoRD1Jy" crossorigin="anonymous"></script>
</body>
</html>


